X	$(CC) $(CFLAGS) -d- -oo\$*.obj -c $*.c
X# must not merge strings, or the tombstone lines will overlap.
Xo\rnd.obj:  $(HACK_H)
Xo\rumors.obj:  $(HACK_H)

Xo\monmove.obj:  $(HACK_H) $(INCL)\mfndpos.h $(INCL)\artifact.h
Xo\monst.obj:  $(CONFIG_H) $(PERMONST_H) $(INCL)\eshk.h $(INCL)\vault.h $(INCL)\epri.h
Xo\msdos.obj:  $(HACK_H)
X	$(CC) $(CFLAGS) -A- -oo\$*.obj -c $*.c
X# set ANSI only off; many MS-DOS specific things.
Xo\mthrowu.obj:  $(HACK_H)
Xo\music.obj:  $(HACK_H)
Xo\o_init.obj:  $(HACK_H) $(INCL)\onames.h

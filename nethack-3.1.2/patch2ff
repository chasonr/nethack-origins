X! #ifdef SUSPEND		/* Does not imply BSD */
X  int
X  dosuspend()
X  {
X! #ifdef SIGTSTP
X  	if(signal(SIGTSTP, SIG_IGN) == SIG_DFL) {
X  		suspend_nhwindows(NULL);
X  		(void) signal(SIGTSTP, SIG_DFL);
X! #ifdef AUX
X  		( void ) kill ( 0 , SIGSTOP ) ;
X! #else
X  		(void) kill(0, SIGTSTP);
